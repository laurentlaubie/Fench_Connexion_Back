{% extends 'admin/base.html.twig' %}

{% block title %}Edition d'un utilisateur
{% endblock %}

{% block body %}
	<h2 class="m-3">Modification d'un utilisateur</h2>
	
	{% form_theme form 'bootstrap_4_layout.html.twig' %}
	<div class="form m-3" style="width: 20rem;">
		{{ form_start(form) }}
		{{ form_widget(form) }}
		<button class="btn btn-info">Modifier</button>
		{{ form_end(form) }}
	</div>
{% endblock %}